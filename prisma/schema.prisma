generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model userHistory {
  id            String   @id @default(uuid())
  walletAddress String
  name          String
  price         Int
  created_at    DateTime @default(dbgenerated("now() at time zone 'utc' + interval '9 hours'"))
  oneShotUrl    String
}
